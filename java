 
 public class SearchInUnicode{
 	public static void main(String agrs[]){
 		char chinaword1='你';char chinaword2='我';char chinaword3='他';
 		System.out.println("汉字："+chinaword1+"的位置："+(int)chinaword1);
 		System.out.println("汉字："+chinaword2+"的位置："+(int)chinaword2);
 		System.out.println("汉字："+chinaword3+"的位置："+(int)chinaword3);
 	}
 }
